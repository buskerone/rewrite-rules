{
  "rewrites": [
    {"source": "/app/", "destination": "https://admin-buskerone-app.vercel.app/"},
    {
      "source": "/app/:slug",
      "has": [
        {
          "type": "query",
          "key": "slug",
          "value": "(?<slug>.*)"
        }
      ],
      "destination": "https://admin-buskerone-app.vercel.app/:slug"
    },
    {"source": "/", "destination": "https://buskerone-app.vercel.app/"},
    {"source": "/:match*", "destination": "https://buskerone-app.vercel.app/:match*"}
  ]
}